{
    "name": "abap2ui5-layout-variant-management",
    "private": true,
    "version": "1.0.0",
    "description": "Layout Management for abap2UI5.",
    "scripts": {
      "syfixes": "find . -type f -name '*.abap' -exec sed -i -e 's/ RAISE EXCEPTION TYPE cx_sy_itab_line_not_found/ ASSERT 1 = 0/g' {} + ",
      "downport": "abaplint --fix ./abaplint-abap_702.jsonc && npm run syfixes",
      "rename": "abaplint abaplint_rename.json --rename"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/abap2UI5-addons/layout-management.git"
    },
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/abap2UI5-addons/layout-management.git"
    },
    "homepage": "https://github.com/abap2UI5-addons/layout-management#readme",
    "devDependencies": {
      "@abaplint/cli": "^2.113.101",
      "@abaplint/database-sqlite": "^2.10.24",
      "@abaplint/runtime": "^2.10.24",
      "@abaplint/transpiler-cli": "^2.10.24"
    },
    "dependencies": {
      "npm-check-updates": "^17.1.14"
    }
  }
